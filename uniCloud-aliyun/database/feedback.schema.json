// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"type": {
			"bsonType": "string",
			"description": "类型"
		},
		"title": {
			"bsonType": "string",
			"description": "反馈标题"
		},
		"describe": {
			"bsonType": "string",
			"description": "描述"
		},
		"img": {
			"bsonType": "array",
			"description": "照片",
			"maxLength": 3 // 限制最大数量
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间"
		}
	}
}
